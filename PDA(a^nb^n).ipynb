    "s=input(\"Enter a String : \")\n",
    "pda=[]\n",
    "c=0\n",
    "for i in s:\n",
    "    if i=='a':\n",
    "        pda.append('a')\n",
    "        c+=1\n",
    "    else:\n",
    "        pda.pop()\n",
    "        c-=1\n",
    "if c==0:\n",
    "    print(\"Valid String\")\n",
    "else:\n",
    "    print(\"Invalid String\")"
